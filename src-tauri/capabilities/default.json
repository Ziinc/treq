{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main", "treq-*"],
  "permissions": [
    "core:default",
    "core:window:allow-create",
    "core:webview:allow-create-webview-window",
    "opener:default",
    "opener:allow-open-url",
    "opener:allow-open-path",
    "dialog:default",
    "dialog:allow-open"
  ]
}
